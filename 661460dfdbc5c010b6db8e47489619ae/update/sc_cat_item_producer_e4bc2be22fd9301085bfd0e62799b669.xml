        <script><![CDATA[current.active = false;
current.title = producer.title;
current.body = producer.content;

producer.tags.toString().split(",").forEach(createTags);

function createTags(tag) {
    tag = tag.trim();
	tag = tag.split(' ').map(function(t) { 
		return t.substr(0, 1).toUpperCase() + t.substr(1, t.length); 
	}).join(' ');
    if (tag.length > 0) {
        var labelSysId;
        var labelGr = new GlideRecord('label');
        labelGr.setLimit(1);
        labelGr.addActiveQuery();
        labelGr.addQuery('name', tag);
        labelGr.query();
        if (!labelGr.next()) {
            var labGr = new GlideRecord('label');
            labGr.initialize();
            labGr.name = tag;
            labelSysId = labGr.insert();
        } else {
            labelSysId = labelGr.getUniqueValue();
        }
        var labelEntryGr = new GlideRecord('label_entry');
        labelEntryGr.initialize();
        labelEntryGr.label = labelSysId;
        labelEntryGr.title = "Did You Know - " + current.number;
        labelEntryGr.table = "x_93700_arena_did_you_know";
        labelEntryGr.table_key = current.sys_id;
		labelEntryGr.id_type = current.getClassDisplayValue();
        labelEntryGr.read = 'yes';
        labelEntryGr.insert();
    }
}


//current.u_approver = gs.getProperty('x_93700_arena.did.you.know.approver');]]></script>
