        <template><![CDATA[<div class="col s12 card-panel white">
  <div class="black-text col s8 l10">
    <h5>{{test.name}}</h5>
    <div class="badge orange lighten-4" >Duration: {{test.duration}} minutes</div>
    <div class="badge blue lighten-4" uib-tooltip="{{'Test opens at ' + test.start_time}}" tooltip-placement="right">
      {{test.appear_anytime === true ? test.total_questions + " questions" : test.time_to_go}}
    </div>
  </div>
  <div class="col s4 l2">
    <a class="col s12 waves-effect black-text btn start-btn blue lighten-5" 
       ng-click="c.validateTest(test)">
      <span ng-show="test.transition">
        <div class="preloader-wrapper active" style="margin: 4px auto;height: 26px;width: 26px;">
          <div class="spinner-layer spinner-blue-only">
            <div class="circle-clipper left">
              <div class="circle"></div>
            </div><div class="gap-patch">
            <div class="circle"></div>
            </div><div class="circle-clipper right">
            <div class="circle"></div>
            </div>
          </div>
        </div>
      </span>
      <span ng-hide="test.transition">
        {{test.enable_retakes === true && test.appeared === true ? 'RETAKE' : 'START'}}
        <i class="material-icons right">navigate_next</i>
      </span>
    </a>
  </div>
</div>]]></template>
    </sp_ng_template>
</record_update>
