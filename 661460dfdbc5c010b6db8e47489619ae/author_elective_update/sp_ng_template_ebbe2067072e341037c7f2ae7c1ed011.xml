        <template><![CDATA[<div ng-style="transition === true && {'filter': 'blur(5px)'}">
  <div class="card z-depth-3 login-form">
    <div class="card-content">
      <form ng-submit="login(username, password, remember_me)">
        <div class="row">
          <div class="input-field col s12">
            <input id="username" type="text" ng-model="username" class="validate" required>
            <label for="username">Username</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input id="password" type="password" ng-model="password" class="validate" required>
            <label for="password">Password</label>
          </div>
        </div>
        <div class="switch">
          <button type="submit" class="waves-effect waves-light btn white-text">LOGIN</button>    
          <button type="button" class="waves-effect waves-light btn white-text" ng-click="goToRegistration()">REGISTER</button>    
          <label class="switch-label">
            Remember me
            <input type="checkbox" ng-model="remember_me">
            <span class="lever"></span>
          </label>
        </div>
      </form>
    </div>
  </div>
</div>]]></template>
    </sp_ng_template>
