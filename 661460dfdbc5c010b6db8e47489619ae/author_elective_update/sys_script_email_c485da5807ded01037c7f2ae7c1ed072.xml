        <script><![CDATA[(function runMailScript( /* GlideRecord */ current, /* TemplatePrinter */ template,
    /* Optional EmailOutbound */
    email, /* Optional GlideRecord */ email_action,
    /* Optional GlideRecord */
    event) {
    var recipientId = event.parm1;
	var userId = "";
	var userPassword = "";
	var credentials = "";

    var userGr = new GlideRecord('sys_user');
    userGr.addQuery('sys_id', recipientId);
    userGr.query();
    if (userGr.next()) {
        credentials+="<div>UserID :        "+userGr.getValue('user_name')+"</div><div>Password :      "+gs.getProperty('x_93700_arena.test.user.default_password');
    }
    template.print(credentials);
})(current, template, email, email_action, event);]]></script>
